<Page x:Class="ExplorandoMarteComTecnologia_WPF.Views.Avaliacao"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ExplorandoMarteComTecnologia_WPF.Views"
      mc:Ignorable="d" 
      d:DesignHeight="768" d:DesignWidth="1366"
      Title="Avaliacao"
      Height="768" Width="1366">

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Imagens/Fundos/Fundo5.jpg"/>
        </Grid.Background>

        <Label x:Name="lblPergunta" Content="lblPergunta" HorizontalAlignment="Left" Margin="608,135,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblMensagem" Content="lblMensagem" HorizontalAlignment="Left" Margin="295,449,0,0" VerticalAlignment="Top"/>

        <Image x:Name="pcbExcelente" HorizontalAlignment="Left" Height="100" Margin="382,218,0,0" VerticalAlignment="Top" Width="100" Source="/Imagens/PlaceHolder/Avaliacao/Botao Excelente.png" Stretch="Fill" MouseDown="pcbExcelente_MouseDown"/>
        <Image x:Name="pcbBom" HorizontalAlignment="Left" Height="100" Margin="508,218,0,0" VerticalAlignment="Top" Width="100" Source="/Imagens/PlaceHolder/Avaliacao/Botao Bom.png" Stretch="Fill" MouseDown="pcbBom_MouseDown"/>
        <Image x:Name="pcbRegular" HorizontalAlignment="Center" Height="100" Margin="0,218,0,0" VerticalAlignment="Top" Width="100" Source="/Imagens/PlaceHolder/Avaliacao/Botao Regular.png" Stretch="Fill" MouseDown="pcbRegular_MouseDown"/>
        <Image x:Name="pcbRuim" HorizontalAlignment="Left" Height="100" Margin="764,218,0,0" VerticalAlignment="Top" Width="100" Source="/Imagens/PlaceHolder/Avaliacao/Botao Ruim.png" Stretch="Fill" MouseDown="pcbRuim_MouseDown"/>
        <Image x:Name="pcbPessimo" HorizontalAlignment="Left" Height="100" Margin="887,218,0,0" VerticalAlignment="Top" Width="100" Source="/Imagens/PlaceHolder/Avaliacao/Botao Pessimo.png" Stretch="Fill" MouseDown="pcbPessimo_MouseDown"/>

        <Image x:Name="pcbContinuar" HorizontalAlignment="Left" Height="94" Margin="1030,598,0,0" VerticalAlignment="Top" Width="237" Source="/Imagens/Botoes/BotaoContinuar.png" MouseDown="pcbContinuar_MouseDown" Stretch="Fill"/>
        <TextBox x:Name="txbComentario" HorizontalAlignment="Left" Height="141" Margin="377,502,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="424"/>

    </Grid>
</Page>
